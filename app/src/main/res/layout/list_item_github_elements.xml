<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    android:descendantFocusability="blocksDescendants"
    android:background="@color/WHITE">

    <TextView
        android:layout_width="wrap_content" android:layout_height="wrap_content"
        android:text="Hello World"
        android:textSize="15sp"
        android:layout_marginTop="16dp"
        android:layout_toRightOf="@+id/imageView"
        android:id="@+id/repository"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:textColor="@color/Rhino"
        >
    </TextView>

    <TextView
        android:layout_width="wrap_content" android:layout_height="wrap_content"
        android:text="Hello World"
        android:textSize="15sp"
        android:id="@+id/description"
        android:layout_marginRight="10dp"
        android:layout_marginTop="4dp"
        android:layout_marginLeft="10dp"
        android:layout_toRightOf="@+id/imageView"
        android:layout_below="@+id/repository"
        android:textColor="@color/color_for_patient_list"
        >
    </TextView>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/code"
        android:layout_toRightOf="@+id/imageView"
        android:layout_below="@+id/description"
        android:focusableInTouchMode="false"
        android:focusable="false"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="14dp"
        android:layout_marginLeft="8dp"
        android:adjustViewBounds="false"
        android:src="@drawable/ic_code_black_24px"
        />
    <TextView
        android:layout_width="wrap_content" android:layout_height="wrap_content"
        android:text="Hello World"
        android:id="@+id/codeText"
        android:textSize="14sp"
        android:layout_marginTop="16dp"
        android:textColor="@color/Rhino"
        android:layout_marginLeft="2dp"
        android:layout_marginRight="16dp"
        android:layout_toRightOf="@+id/code"
        android:layout_below="@+id/description"
        >
    </TextView>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/bug"
        android:layout_toRightOf="@+id/codeText"
        android:layout_below="@+id/description"
        android:focusableInTouchMode="false"
        android:focusable="false"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="14dp"
        android:layout_marginLeft="8dp"
        android:adjustViewBounds="false"
        android:src="@drawable/ic_bug_report_black_24px"
        />
    <TextView
        android:layout_width="wrap_content" android:layout_height="wrap_content"
        android:text="Hello World"
        android:id="@+id/bugtext"
        android:textSize="14sp"
        android:layout_marginTop="16dp"
        android:textColor="@color/Rhino"
        android:layout_marginLeft="2dp"
        android:layout_marginRight="16dp"
        android:layout_toRightOf="@+id/bug"
        android:layout_below="@+id/description"
        >
    </TextView>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/watcher"
        android:layout_toRightOf="@+id/bugtext"
        android:layout_below="@+id/description"
        android:focusableInTouchMode="false"
        android:focusable="false"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="14dp"
        android:layout_marginLeft="8dp"
        android:adjustViewBounds="false"
        android:src="@drawable/ic_face_black_24px"
        />
    <TextView
        android:layout_width="wrap_content" android:layout_height="wrap_content"
        android:text="Hello World"
        android:id="@+id/watcher_count"
        android:textSize="14sp"
        android:layout_marginTop="16dp"
        android:textColor="@color/Rhino"
        android:layout_marginLeft="2dp"
        android:layout_marginRight="16dp"
        android:layout_toRightOf="@+id/watcher"
        android:layout_below="@+id/description"
        >
    </TextView>

    <ImageView
        android:layout_width="44dp"
        android:layout_height="44dp"
        android:id="@+id/imageView"
        android:scaleType="centerCrop"
        android:src="@drawable/ic_book_black_24px"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />



</RelativeLayout>
